package coreJava;

public class forlooppattern {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int k=1;
		int l=0;
		for(int i=1;i<5;i++)
		{
			System.out.println(l);
			for(int j=1;j<=i;j++)
			{
				System.out.print(k);
				System.out.print("\t");
				
			}
			System.out.println(" ");
		}

	}

	

}

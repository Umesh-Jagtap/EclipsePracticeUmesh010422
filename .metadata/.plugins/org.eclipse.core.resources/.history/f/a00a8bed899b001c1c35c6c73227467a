package coreJava;

public class forDemo {

	public static void main(String[] args) {
		int k=0;
		// TODO Auto-generated method stub
		for(int i=3;i<20;i=i+3)
		{
			for(int j=3;j<i;j=j+3)
			System.out.print(j);
			System.out.println("\t");
			
		}
		System.out.println(" ");
		
		/*for(int i=0;i<4;i++)
		{
			for(int j=1;j<=4-i;j++)
			{
				System.out.print(k);
				System.out.print("\t");
				k++;
			}
			System.out.println(" ");
		}*/
		

	}

}
